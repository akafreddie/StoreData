<!DOCTYPE html>
<html>
<head>
    <title>mLab MongoDB test</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<div class="container">
    <h2>Database Results: </h2>
    <ul>
        <% results.forEach(function(r) { %>
        <li><%= r.name %> - <%= r.frequency %></li>
        <ul>
            <li>Billing & ID: <%=r.BILLING_ID %>, <%=r.CUSTOMER_ID %> </li>
            <li>Date & Total Order: ( <%=r.DATE %>,<%=r.ORDER_TOTAL %> </li>
        </ul>
        <br/><br/>
        <% }); %>
    </ul>
</div>

</body>
</html>

