<!DOCTYPE html>
<html>
<head>
    <title>mLab MongoDB test</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<div class="container">
    <h2>Database Results: </h2>
    <ul>
        <% results.forEach(function(r) { %>
        <li><%= r.name %> - <%= r.frequency %></li>
        <ul>
            <li>Customer ID: <%=r.CUSTOMER_ID %> </li>
            <li>Billing ID: <%=r.BILLING_ID %> </li>
            <li>Shipping ID: <%=r.BILLING_ID%></li>
            <li>DATE: <%=r.DATE%></li>
            <li>Product Vector: <%r.PRODUCT_VECTOR%></li>
            <li>Order Total: <%r.ORDER_TOTAL%></li>
        </ul>
        <br/><br/>
        <% }); %>
    </ul>
</div>

</body>
</html>

